// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "dbg/*" : true,
    "dbg64/*" : true,
    "rel/*" : true,
    "rel64/*" : true,
    "relPrefast/*" : true,
    "relPrefast64/*" : true,
    "build-log.txt" : true
  },
  "python.linting.pylintEnabled": false,
  "files.associations": {
    "xstring": "cpp",
    "xhash": "cpp",
    "xtree": "cpp",
    "xutility": "cpp",
    "array": "cpp",
    "initializer_list": "cpp",
    "list": "cpp",
    "type_traits": "cpp",
    "vector": "cpp",
    "xlocale": "cpp",
    "*.rh": "cpp"
  }
}
